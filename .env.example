# 环境配置（复制此文件为 .env 并填入实际值）

# 运行环境
NODE_ENV=production
PORT=3001

# MySQL 数据库配置
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=your_mysql_password
MYSQL_DATABASE=ai_park

# Redis 配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT 配置
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRE=7d

# API 地址配置
API_BASE_URL=http://localhost:3001
VITE_API_BASE_URL=http://localhost:3001/api
